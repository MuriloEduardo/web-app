generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
}

model Healthcheck {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
}

model User {
  id           String   @id @default(cuid())
  email        String   @unique
  phone_number String?
  passwordHash String
  name         String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
